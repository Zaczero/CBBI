{
    "configurations": [
        {
            "console": "integratedTerminal",
            "name": "Python Debugger: Current File",
            "program": "${file}",
            "request": "launch",
            "type": "debugpy"
        },
        {
            "console": "integratedTerminal",
            "justMyCode": true,
            "name": "Python: main.py",
            "program": "${workspaceFolder}/main.py",
            "request": "launch",
            "type": "debugpy"
        }
    ],
    "version": "0.2.0"
}
